<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="SwagLabsSuite" verbose="1">
    <test name="SmokeTest">
        <groups>
            <run>
                <include name="smoke"/>
            </run>
        </groups>
        <classes>
            <class name="com.swaglabs.tests.LoginTests"/>
            <class name="com.swaglabs.tests.ProductsTests"/>
        </classes>
    </test>
    <test name="RegressionTest">
        <groups>
            <run>
                <include name="regression"/>
            </run>
        </groups>
        <classes>
            <class name="com.swaglabs.tests.CartTests"/>
            <class name="com.swaglabs.tests.CheckoutTest"/>
        </classes>
    </test>
</suite>